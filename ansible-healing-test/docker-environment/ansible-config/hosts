[ubuntu_servers]
ubuntu-server ansible_host=ubuntu-server ansible_user=testuser ansible_password=testpass ansible_become=yes

[centos_servers]
centos-server ansible_host=centos-server ansible_user=testuser ansible_password=testpass ansible_become=yes

[alpine_servers]
alpine-server ansible_host=alpine-server ansible_user=testuser ansible_password=testpass ansible_become=yes

[all_servers:children]
ubuntu_servers
centos_servers
alpine_servers

[all_servers:vars]
ansible_python_interpreter=/usr/bin/python3
